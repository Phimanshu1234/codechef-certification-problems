#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,n,i,c1,c2;
    string s1,s2;
    cin>>t;
    while(t--)
    {c1=0;
      c2=0;
        cin>>n;
        cin>>s1;
        cin>>s2;
        for(i=0;i<n;i++)
        {
            if(s1[i]=='1')
            {
                c1++;
            }
            if(s2[i]=='1')
            {
                c2++;
            }
        }
        if(c1==c2)
        {
            printf("YES\n");
        }
        else
        {
            printf("NO\n");
        }
    }
    return 0;
}
