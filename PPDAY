#include<stdio.h>
#include<string.h>
int main()
{
   int t,k,l,i;
   scanf("%d",&t);
   char str[100];
   while(t--)
   {k=0;
        scanf("%s",str);
       l=strlen(str);
       for(i=0;i<l;i=i+2)
        {
            if(str[i]==str[i+1])
            {
                k++;
            }
            else
            {
                k=0;
            }
        }
     
       if(k==0)
       {
            printf("yes\n");
       }
        else
        {
         printf("no\n");
        }
   }
    return 0;
}
