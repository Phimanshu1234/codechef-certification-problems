
#include<bits/stdc++.h>
using namespace std;
int main()
{
      long long int t,l,i;
     char str[20];
     char d;
    cin>>t;
    vector <char> a;
    while(t--)
    {
        cin>>str;
        for(i=0;i<strlen(str);i++)
        {
            a.push_back(str[i]);
        }
        cin>>d;
        l=strlen(str);
        for(i=l-1;i>=0;i--)
        {
          if(a[i]>d)
          {
              a.erase(a.begin()+i);
              a.push_back(d);
          }
        }
        for(i=0;i<l-1;i++)
        {
            if(a[i]>a[i+1])
            {
                a.erase(a.begin()+i);
              a.push_back(d);
            }
        }
        for(i=0;i<l-1;i++)
        {
            if(a[i]>a[i+1])
            {
                a.erase(a.begin()+i);
              a.push_back(d);
            }
        }
        for(i=0;i<l-1;i++)
        {
            if(a[i]>a[i+1])
            {
                a.erase(a.begin()+i);
              a.push_back(d);
            }
        }
        for(i=0;i<l-1;i++)
        {
            if(a[i]>a[i+1])
            {
                a.erase(a.begin()+i);
              a.push_back(d);
            }
        }
        for(i=0;i<l-1;i++)
        {
            if(a[i]>a[i+1])
            {
                a.erase(a.begin()+i);
              a.push_back(d);
            }
        }
        for(i=0;i<l-1;i++)
        {
            if(a[i]>a[i+1])
            {
                a.erase(a.begin()+i);
              a.push_back(d);
            }
        }
        for(i=0;i<l-1;i++)
        {
            if(a[i]>a[i+1])
            {
                a.erase(a.begin()+i);
              a.push_back(d);
            }
        }
        for(i=0;i<l-1;i++)
        {
            if(a[i]>a[i+1])
            {
                a.erase(a.begin()+i);
              a.push_back(d);
            }
        }
        for(i=0;i<l-1;i++)
        {
            if(a[i]>a[i+1])
            {
                a.erase(a.begin()+i);
              a.push_back(d);
            }
        }
        for(i=0;i<l-1;i++)
        {
            if(a[i]>a[i+1])
            {
                a.erase(a.begin()+i);
              a.push_back(d);
            }
        }
        for(i=0;i<l-1;i++)
        {
            if(a[i]>a[i+1])
            {
                a.erase(a.begin()+i);
              a.push_back(d);
            }
        }
        for(i=0;i<l-1;i++)
        {
            if(a[i]>a[i+1])
            {
                a.erase(a.begin()+i);
              a.push_back(d);
            }
        }
        
        for(i=0;i<strlen(str);i++)
        {
            cout<<a[i];
        }
        cout<<endl;
        a.clear();
    }
    return 0;
}
