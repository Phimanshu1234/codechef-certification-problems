#include<bits/stdc++.h>
using namespace std;
#define lld long long int
#define speed ios_base::sync_with_stdio(0); cin.tie(0);cout.tie(0);
#define M 1000000007
int main()
{
    speed
    lld n,l,r,s,i;
    cin>>n;
     lld a[102]={0};
     for(i=7;i<=100;i=i+10)
     {
         a[i]=1;
     }
     for(i=71;i<=79;i++)
     {
         a[i]=1;
     }
    while(n--)
    {s=0;
        cin>>l>>r;
        for(i=l;i<=r;i++)
        {
            s+=a[i];
        }
        cout<<s<<"\n";
    }
    return 0;
}
