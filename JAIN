#include <stdio.h>
#include<string.h>
int main()
{
   int t,n,i,j,k,sum,a,b,c,d,e,a1,b1,c1,d1,e1;
   char str[100001][1001];
   scanf("%d",&t);
   while(t--)
   {sum=0;
       scanf("%d",&n);
       for(i=0;i<n;i++)
       {
           scanf("%s",str[i]);
       }
       for(i=0;i<n-1;i++)
       {
           a=b=c=d=e=0;
         for(j=0;j<strlen(str[i]);j++)
         {
             if(str[i][j]=='a')
             {
                 a++;
             }
             if(str[i][j]=='e')
             {
                 b++;
             }
             if(str[i][j]=='i')
             {
                 c++;
             }
             if(str[i][j]=='o')
             {
                 d++;
             }
             if(str[i][j]=='u')
             {
                 e++;
             }
         }
         for(j=i+1;j<n;j++)
         {
             a1=b1=c1=d1=e1=0;
            for(k=0;k<strlen(str[j]);k++)
            {
               if(str[j][k]=='a')
               {
                   a1++;
               }
               if(str[j][k]=='e')
               {
                   b1++;
               }
               if(str[j][k]=='i')
               {
                   c1++;
               }
               if(str[j][k]=='o')
               {
                   d1++;
               }
               if(str[j][k]=='u')
               {
                   e1++;
               }
            }
            if((a+a1)>=1 && (b+b1)>=1 && (c+c1)>=1 && (d+d1)>=1 && (e+e1)>=1)
         {
             sum++;
         }
         }
       }
       printf("%d\n",sum);
   }
   return 0;
}
