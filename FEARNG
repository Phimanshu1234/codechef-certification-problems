#include<bits/stdc++.h>
using namespace std;
#define lld long long int
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    lld t,n;
    cin>>t;
    while(t--)
    {
        cin>>n;
        n=n*2;
       lld k=log2(n);
        //cout<<k;
       lld p=pow(2,k);
       //cout<<p<<"\n";
        n=n-p;
        n++;
        cout<<n<<"\n";
    }
    return 0;
}
