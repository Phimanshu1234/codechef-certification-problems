#include<bits/stdc++.h>
using namespace std;
#define lld long long int
#define speed ios_base::sync_with_stdio(0); cin.tie(0);cout.tie(0);
#define M 1000000007
int main()
{
    speed
    
    lld n,i,k1=0,k2=0;
    cin>>n;
    lld a[n+1];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
        if(a[i]%2==0)
        {
            k1++;
        }
        else
        {
            k2++;
        }
    }
    if(abs(k1-k2)>1)
    {
        cout<<"DETAIN";
    }
    else
    {
        cout<<"DL";
    }
    return 0;
}
